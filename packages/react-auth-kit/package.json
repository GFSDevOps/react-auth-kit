{
  "name": "react-auth-kit",
  "version": "3.0.0-alpha.6",
  "description": "Authentication Library for React, which makes Token based auth very easy",
  "keywords": [
    "react",
    "auth",
    "authentication",
    "jwt",
    "jsonwebtoken",
    "token"
  ],
  "author": "Arkadip Bhattacharya <in2arkadipb13@gmail.com>",
  "homepage": "https://authkit.arkadip.dev",
  "license": "MIT",
  "source": "src/index.tsx",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "src",
    "higherOrderComponents",
    "hooks",
    "utils",
    "*.d.ts*",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-auth-kit/react-auth-kit.git"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rimraf ./dist",
    "build:dir": "mkdirp ./dist",
    "build:build": "tsc --project tsconfig.json && babel --config-file ./babel.config.js src --out-dir . --extensions \".tsx,.ts,.js,.jsx\"",
    "preversion": "npm run test",
    "test": "run-s test:*",
    "test:build": "run-s build",
    "test:unit": "jest",
    "test:lint": "eslint src/**/*.ts src/**/*.tsx --fix",
    "test:unit:watch": "jest --watch"
  },
  "bugs": {
    "url": "https://github.com/react-auth-kit/react-auth-kit/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-optional-catch-binding": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.5",
    "@types/js-cookie": "3.0.4",
    "@types/node": "18.18.3",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "eslint": "8.50.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-react": "7.33.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.4.3",
    "mkdirp": "3.0.1",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.5",
    "rollup": "3.29.4",
    "rollup-plugin-filesize": "10.0.0",
    "rollup-plugin-license": "3.1.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.36.0",
    "ts-jest": "29.1.1"
  },
  "dependencies": {
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "react": ">=16.8.0 || ^17.x || ^18.x",
    "react-router-dom": "^6.2.0"
  },
  "peerDependencies": {
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "react": ">=16.8.0 || ^17.x || ^18.x",
    "react-dom": ">=16.8.0 || ^17.x || ^18.x",
    "react-router-dom": "^6.2.0"
  },
  "gitHead": "d29d7d65ef13ce1daebe4785400c9bc5cab06e02"
}
